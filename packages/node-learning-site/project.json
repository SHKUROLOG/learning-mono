{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "packages/node-learning-site/src",
  "projectType": "application",
  "targets": {
    "seed": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "packages/node-learning-site",
        "command": "ts-node --project tsconfig.app.json src/seed.ts"
      }
    },
    "prisma-format": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn prisma format --schema packages/node-learning-site/schema.prisma"
      }
    },
    "prisma-studio": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn prisma studio --schema packages/node-learning-site/schema.prisma"
      }
    },
    "prisma-push": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn prisma db push --force-reset --schema packages/node-learning-site/schema.prisma"
      }
    },
    "prisma-generate": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn prisma generate --schema packages/node-learning-site/schema.prisma"
      },
      "configurations": {
        "watch": {
          "command": "yarn prisma generate --watch --schema packages/node-learning-site/schema.prisma"
        }
      }
    },
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/node-learning-site",
        "main": "packages/node-learning-site/src/main.ts",
        "tsConfig": "packages/node-learning-site/tsconfig.app.json",
        "assets": [
          "packages/node-learning-site/src/assets"
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/node-learning-site/src/environments/environment.ts",
              "with": "packages/node-learning-site/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "node-learning-site:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/node-learning-site/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/node-learning-site"
      ],
      "options": {
        "jestConfig": "packages/node-learning-site/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
